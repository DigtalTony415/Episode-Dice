import javax.swing.*;


    public class EpisodeDice
   {
   
   
    public static void main (String[] args)
    {

        JFrame frame = new JFrame ("Episode Selector");
        frame.setDefaultCloseOperation (JFrame.EXIT_ON_CLOSE);

        frame.getContentPane().add(new DicePanel());
        frame.pack();
        //frame.setLocationRelativeTo(center);
        frame.setLocation(540, 320);
        frame.setVisible(true);
        
    }
    
}
